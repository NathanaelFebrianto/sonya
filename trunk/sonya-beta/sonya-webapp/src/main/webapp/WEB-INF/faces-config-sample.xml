  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->
<faces-config>
    <!-- Add beans here -->
    <managed-bean>
        <managed-bean-name>sample$Page1</managed-bean-name>
        <managed-bean-class>com.sonya.webapp.sample.Page1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>personManager</property-name>
            <value>#{personManager}</value>
        </managed-property>
    </managed-bean>
    
    <!-- Add additional rules here -->
</faces-config>
