<?xml version="1.0" encoding="UTF-8"?>
<classpath>
	<!--  
	<classpathentry kind="src" path="src/main/java"/>
	<classpathentry kind="src" path="src/main/resources"/>
	<classpathentry kind="src" output="target/test-classes" path="src/test/java"/>
	<classpathentry kind="src" output="target/test-classes" path="src/test/resources"/>
	-->
	<classpathentry kind="con" path="org.eclipse.jdt.launching.JRE_CONTAINER"/>
	<classpathentry kind="con" path="org.maven.ide.eclipse.MAVEN2_CLASSPATH_CONTAINER"/>
	<classpathentry kind="output" path="target/classes"/>

	<!-- sonya-base -->
	<classpathentry kind="src" path="sonya-basic/src/main/java" output="sonya-basic/target/classes"/>
	<classpathentry kind="src" path="sonya-basic/src/main/resources" excluding="**/*.java" output="sonya-basic/target/classes"/>
	<classpathentry kind="src" path="sonya-basic/src/main/webapp" output="sonya-basic/target/test-classes" including="**/*.xml" excluding="**/*.java"/>
	<classpathentry kind="src" path="sonya-basic/src/test/java" output="sonya-basic/target/test-classes"/>
	<classpathentry kind="src" path="sonya-basic/src/test/resources" output="sonya-basic/target/test-classes" excluding="**/*.java"/>
	
	<!-- sonya-sample -->
	<classpathentry kind="src" path="sonya-sample/src/main/java" output="sonya-sample/target/classes"/>
	<classpathentry kind="src" path="sonya-sample/src/main/resources" excluding="**/*.java" output="sonya-sample/target/classes"/>
	<classpathentry kind="src" path="sonya-sample/src/main/webapp" output="sonya-sample/target/test-classes" including="**/*.xml" excluding="**/*.java"/>
	<classpathentry kind="src" path="sonya-sample/src/test/java" output="sonya-sample/target/test-classes"/>
	<classpathentry kind="src" path="sonya-sample/src/test/resources" output="sonya-sample/target/test-classes" excluding="**/*.java"/>
</classpath>
